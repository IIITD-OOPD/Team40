<circuit name="Full Adder">
    
	<input id='1' name='A'> </input>
	<input id='2' name='B'> </input>
	<input id='3' name='C'> </input>
	
	
	<gate id='1' type='xor'> 
	  <input type='input' id='1'> </input>
	  <input type='input' id='2'> </input>
	</gate>
	
	<gate id='2' type='xor'> 
	  <input type='input' id='3'> </input>
	  <input type='gate' id='1'> </input>
	</gate>
	
	<gate id='3' type='and'> 
	  <input type='input' id='1'> </input>
	  <input type='input' id='2'> </input>
	</gate>
	
	<gate id='4' type='and'> 
	  <input type='input' id='3'> </input>
	  <input type='gate' id='1'> </input>
	</gate>
	
	<gate id='5' type='or'> 
	  <input type='gate' id='4'> </input>
	  <input type='gate' id='5'> </input>
	</gate>
	
	<output id='1'>
	   <input type='gate' id='2'> </input>
	</output>
	
	<output id='2'>
	   <input type='gate' id='5'> </input>
	</output>
	
</circuit>