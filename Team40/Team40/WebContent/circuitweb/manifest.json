{
    "name": "Quoracle",
    "version": "1.0",

    "manifest_version": 2,
   
	"browser_action": {
		"default_popup": "popup.html",
		"default_icon": "Q_LOGOweb.png" 
    },
	
	"content_scripts": [
    	{
        "matches": ["*://*.quora.com/*"],
        "js": ["jquery.js","content.js"]
    	}
	],
	
	"permissions":["*://*.quora.com/*","http://*/"],
	
	"web_accessible_resources": ["tick.png","tick1.png","Q_LOGOweb.png"],
	
	
	
	 "background": {
        "scripts": ["jquery.js","background.js"],
        "persistent": false
    }
	
}